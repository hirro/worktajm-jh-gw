<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Project.
    -->
    <changeSet id="20170525091355-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="worker_id"
                                 baseTableName="project"
                                 constraintName="fk_project_worker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="worker"/>

        <addForeignKeyConstraint baseColumnNames="time_entry_id"
                                 baseTableName="project"
                                 constraintName="fk_project_time_entry_id"
                                 referencedColumnNames="id"
                                 referencedTableName="time_entry"/>

    </changeSet>
</databaseChangeLog>
